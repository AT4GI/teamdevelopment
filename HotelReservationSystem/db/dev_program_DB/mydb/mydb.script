CREATE SCHEMA PUBLIC AUTHORIZATION DBA
CREATE MEMORY TABLE ROOM(ROOMNUMBER VARCHAR(50) NOT NULL PRIMARY KEY,ROOM_TYPE VARCHAR(20),PRICE INTEGER,STAYINGDATE VARCHAR(20))
CREATE MEMORY TABLE RESERVATION(RESERVATIONNUMBER VARCHAR(50) NOT NULL PRIMARY KEY,CHECKIN_DATE VARCHAR(20),CHECKOUT_DATE VARCHAR(20),ROOM_TYPE VARCHAR(20),STATUS VARCHAR(10))
CREATE MEMORY TABLE AVAILABLEQTY(STAY_DATE VARCHAR(20) NOT NULL,ROOM_TYPE VARCHAR(20) NOT NULL,QTY INTEGER,PRIMARY KEY(STAY_DATE,ROOM_TYPE))
CREATE MEMORY TABLE PAYMENT(ROOMNUMBER VARCHAR(50) NOT NULL,STAYINGDATE VARCHAR(20) NOT NULL,AMOUNT INTEGER,STATUS VARCHAR(10),CONSTRAINT ROOMNUMBER_STAYINGDATE PRIMARY KEY(ROOMNUMBER,STAYINGDATE))
